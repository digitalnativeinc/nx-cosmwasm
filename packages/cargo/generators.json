{
	"$schema": "http://json-schema.org/schema",
	"name": "cargo",
	"version": "0.0.1",
	"generators": {
		"init": {
			"factory": "./src/generators/init/generator",
			"schema": "./src/generators/init/schema.json",
			"description": "Initialize the @digitalnative/cosmwasm plugin",
			"hidden": true
		},
		"cosmwasm": {
			"factory": "./src/generators/osmosis/generator",
			"schema": "./src/generators/osmosis/schema.json",
			"description": "Create a Cosmwasm smart contract from Osmosis",
			"aliases": ["osmosis"]
		}
	}
}
